{
	"version": "2",
	"checks": {
		"file-lines": {
			"config": {
				"threshold": 350
			}
		},
		"method-lines": {
			"config": {
				"threshold": 50
			}
		},
		"method-complexity": {
			"config": {
				"threshold": 10
			}
		},
		"method-count": {
			"config": {
				"threshold": 30
			}
		}
	},
	"exclude_patterns": ["**/*.spec.js", "**/*.karma.js", "src/UI/**/*.*"]
}
